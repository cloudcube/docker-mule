        <http:inbound-endpoint exchange-pattern="request-response" host="localhost" port="8081" path="test_b" encoding="UTF-8" contentType="application/json" doc:name="MuleDockerJavaTestAEndpoint"/>
        <component class="net.cime.mule.docker.TestA" doc:name="DockerTestA"/>
        <json:object-to-json-transformer doc:name="Object to JSON"/>
